<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.neladyn.controller.OtherController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <GridPane maxHeight="100.0" minHeight="100.0" prefHeight="100.0" VBox.vgrow="ALWAYS">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <TextField fx:id="nameField" GridPane.columnIndex="1" />
                    <TextField fx:id="timestampField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <TextField fx:id="valueField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    <Label text="Name" />
                    <Label text="Timestamp" GridPane.rowIndex="1" />
                    <Label text="Value" GridPane.rowIndex="2" />
                </children>
            </GridPane>
            <StackPane fx:id="stackPane" prefHeight="150.0" prefWidth="200.0" />
        </children>
    </VBox>
</AnchorPane>
